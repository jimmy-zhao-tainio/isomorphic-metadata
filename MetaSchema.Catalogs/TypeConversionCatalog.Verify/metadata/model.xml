<?xml version="1.0" encoding="utf-8"?>
<Model name="TypeConversionCatalog">
  <Entities>
    <Entity name="ConversionImplementation">
      <Properties>
        <Property name="CSharpEntryPoint" />
        <Property name="Key" />
        <Property name="Kind" />
      </Properties>
    </Entity>
    <Entity name="DataType">
      <Properties>
        <Property name="Category" isRequired="false" />
        <Property name="Name" />
      </Properties>
      <Relationships>
        <Relationship entity="TypeSystem" />
      </Relationships>
    </Entity>
    <Entity name="DataTypeFacet">
      <Properties>
        <Property name="DefaultBool" dataType="bool" isRequired="false" />
        <Property name="DefaultInt" dataType="int" isRequired="false" />
        <Property name="IsRequired" dataType="bool" />
        <Property name="IsSupported" dataType="bool" />
      </Properties>
      <Relationships>
        <Relationship entity="DataType" />
        <Relationship entity="Facet" />
      </Relationships>
    </Entity>
    <Entity name="Facet">
      <Properties>
        <Property name="Name" />
        <Property name="ValueKind" />
      </Properties>
    </Entity>
    <Entity name="Setting">
      <Properties>
        <Property name="DefaultValue" />
        <Property name="Name" />
      </Properties>
    </Entity>
    <Entity name="TypeMapping">
      <Properties>
        <Property name="IsImplicit" dataType="bool" />
        <Property name="Lossiness" />
        <Property name="Name" />
        <Property name="Notes" isRequired="false" />
        <Property name="Priority" dataType="int" />
      </Properties>
      <Relationships>
        <Relationship entity="ConversionImplementation" />
        <Relationship entity="Setting" />
        <Relationship entity="DataType" role="SourceDataType" />
        <Relationship entity="TypeSystem" role="SourceTypeSystem" />
        <Relationship entity="DataType" role="TargetDataType" />
        <Relationship entity="TypeSystem" role="TargetTypeSystem" />
      </Relationships>
    </Entity>
    <Entity name="TypeMappingCondition">
      <Properties>
        <Property name="Operator" />
        <Property name="ValueInt" dataType="int" isRequired="false" />
      </Properties>
      <Relationships>
        <Relationship entity="Facet" />
        <Relationship entity="TypeMapping" />
      </Relationships>
    </Entity>
    <Entity name="TypeMappingFacetTransform">
      <Properties>
        <Property name="Mode" />
        <Property name="SetBool" dataType="bool" isRequired="false" />
        <Property name="SetInt" dataType="int" isRequired="false" />
      </Properties>
      <Relationships>
        <Relationship entity="Facet" />
        <Relationship entity="TypeMapping" />
      </Relationships>
    </Entity>
    <Entity name="TypeSpec">
      <Properties>
        <Property name="IsFixedLength" dataType="bool" isRequired="false" />
        <Property name="IsUnicode" dataType="bool" isRequired="false" />
        <Property name="Length" dataType="int" isRequired="false" />
        <Property name="Precision" dataType="int" isRequired="false" />
        <Property name="Scale" dataType="int" isRequired="false" />
        <Property name="TimePrecision" dataType="int" isRequired="false" />
      </Properties>
      <Relationships>
        <Relationship entity="DataType" />
      </Relationships>
    </Entity>
    <Entity name="TypeSystem">
      <Properties>
        <Property name="Name" />
      </Properties>
    </Entity>
  </Entities>
</Model>
